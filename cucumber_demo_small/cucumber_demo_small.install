<?php

/**
 * @file
 * Install functions for the Cucumber Demo Small module.
 */

/**
 * Implements hook_enable().
 */
function cucumber_demo_small_install() {
  $moduleHandler = \Drupal::service('module_handler');
  if (!$moduleHandler->moduleExists('cucumber_project')) {
      $moduleHandler->install(['cucumber_project']);
  }
}